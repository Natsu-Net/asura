{
  "lock": false,
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ --unstable-kv --unstable-cron --env=.env dev.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "build": "deno run -A --unstable-kv --unstable-cron main.ts",
    "preview": "deno run -A --unstable-kv --unstable-cron main.ts",
    "build-database": "deno run -A --unstable-kv --env=.env build-database.ts",
    "test-kv": "deno run -A --unstable-kv --env=.env test-kv.ts",
    "test-basic": "deno run -A --unstable-kv --env=.env test-basic.ts",
    "check-env": "deno run -A --unstable-kv --env=.env check-env.ts"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.5.11",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/",
    "$std/": "https://deno.land/std@0.224.0/",
    "@twind/core": "https://esm.sh/@twind/core@1.1.3",
    "@twind/preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4/",
    "@twind/preset-autoprefix": "https://esm.sh/@twind/preset-autoprefix@1.0.7/"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*", "transfer.ts", "build-css.ts", "postcss.config.ts"]
}
